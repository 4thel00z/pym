#. Tag: para
#: operatorsexpressions.xml:93
#, no-c-format
msgid "One example code used to show expressions"
msgstr ""

#. Tag: screen
#: operatorsexpressions.xml:96
#, no-c-format
msgid ""
"<![CDATA[\n"
"#!/usr/bin/env python\n"
"a = 9\n"
"b = 12\n"
"c = 3\n"
"x = a -b / 3 + c * 2 -1\n"
"y = a -b / (3 + c) * (2 -1)\n"
"z = a - (b / (3 +c) * 2) -1\n"
"print \"X = \", x\n"
"print \"Y = \", y\n"
"print \"Z = \", z\n"
"]]>"
msgstr ""

#. Tag: screen
#: operatorsexpressions.xml:100
#, no-c-format
msgid ""
"<![CDATA[\n"
"[kd@kdlappy book]$ ./evaluationexp.py\n"
