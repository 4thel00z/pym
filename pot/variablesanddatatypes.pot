#. Tag: title
#, no-c-format
msgid "Average of N numbers"
msgstr ""

#. Tag: para
#, no-c-format
msgid "In the next program we will do an average of N numbers."
msgstr ""

#. Tag: programlisting
#, no-c-format
msgid "#!/usr/bin/env python\n"
"N = 10\n"
"sum = 0\n"
"count = 0\n"
"while count &lt; N:\n"
"    number = float(raw_input(\"\"))\n"
"    sum = sum + number\n"
"    count = count + 1\n"
"average = float(sum)/N\n"
"print \"N = %d , Sum = %f\" % (N, sum)\n"
"print \"Average = %f\" % average\n"
"\n"
""
msgstr ""

