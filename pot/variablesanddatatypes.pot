#, no-c-format
msgid "\n"
"\n"
"&gt;&gt;&gt; a , b = 45, 54\n"
"&gt;&gt;&gt; a\n"
"45\n"
"&gt;&gt;&gt; b\n"
"54\n"
"\n"
""
msgstr ""

